@model db.bks_Supplier

@{
	ViewBag.Title = "Insert";
}

@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
	@Html.ValidationSummary(true)
    
	<div class="dialog">
        <div class="editor-label">
			@Html.LabelFor(model => model.supplierCode)
		</div>
		<div class="editor-field">	
			@Html.EditorFor(model => model.supplierCode)
			@Html.ValidationMessageFor(model => model.supplierCode)
        </div>

        <div class="editor-label">
			@Html.LabelFor(model => model.supplierName)
		</div>
		<div class="editor-field">	
			@Html.EditorFor(model => model.supplierName)
			@Html.ValidationMessageFor(model => model.supplierName)
        </div>

        <div class="editor-label">
			@Html.LabelFor(model => model.contactUser)
		</div>
		<div class="editor-field">	
			@Html.EditorFor(model => model.contactUser)
			@Html.ValidationMessageFor(model => model.contactUser)
        </div>

        <div class="editor-label">
			@Html.LabelFor(model => model.contactPhone)
		</div>
		<div class="editor-field">	
			@Html.EditorFor(model => model.contactPhone)
			@Html.ValidationMessageFor(model => model.contactPhone)
        </div>

        <div class="editor-label">
			@Html.LabelFor(model => model.remark)
		</div>
		<div class="editor-field">	
			@Html.EditorFor(model => model.remark)
			@Html.ValidationMessageFor(model => model.remark)
        </div>

        <p class="button">
            <a class="billSave opSave" data-close="true">保存</a>
            <a class="opClose">关闭</a>
        </p>
    </div>
}
